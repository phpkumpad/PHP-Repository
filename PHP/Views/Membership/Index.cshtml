@model IEnumerable<MemberPcp>

@{
    ViewBag.title = "Membership Information";
    Layout = "_AdminLayout";
}


<table class="table table-striped table-bordered table-sm">
    <tr>
        <th>Member ID</th>
        <th>Member Name</th>
        <th>PCP Group TIN</th>
        <th>PCP Group Name</th>
        <th>PCP NPI</th>
        <th>PCP Name</th>
     <th>Actions</th>
    </tr>

    @foreach (var m in Model)
    {
        <tr>
            <td>@m.MemberId</td>
            <td>@m.MemberName</td>
            <td>@m.PcpgrpTaxId</td>
            <td>@m.PcpgrpName</td>
            <td>@m.PcpNpi</td> 
            <td>@m.PcpName</td>
           
            
            <td class="text-center">
                <form asp-action="Delete" method="post">
                    <a asp-action="Edit" class="btn btn-sm btn-warning" asp-route-memberPk="@m.Pk">
                        Edit
                    </a>
                    <input type="hidden" name="memberPk" value="@m.Pk"/>
                    <button type="submit" class="btn btn-danger btn-sm">Delete</button>
                </form>
            </td>

        </tr>
    }

</table>

<div class="text-center">
    <a asp-action="Create" class="btn btn-primary">Add New Member Assigment</a>
</div>


